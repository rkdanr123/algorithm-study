#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);
    int n = N % 5;
    int m = N / 5;
    if (N==4|N==7){
        printf("%d", -1);
        return 0;
    }
    switch(n) {
        case 0:
            printf("%d", m);
            break;
        case 1:
            printf("%d", m + 1);
            break;
        case 2:
            printf("%d", m + 2);
            break;
        case 3:
            printf("%d", m + 1);
            break;
        case 4:
            printf("%d", m + 2);
            break;
    }
    return 0;
}
